namespace Titan.TrinityScene;

table unknown_sceneObject_data{
	unk1: Vec3f;
	unk2: float;
}
table additional_SRT_Data{
	unk0: uint32; // never set
	name_of_used_vec3f: string;
	unk2: SRT;
}
table trinity_SceneObject {
